function validateDeleteRowIntolandRentTable(tab_str, rowno, cols)
{

	if(deleteFlagLandRent==1 && modifyFlagLandRent==0)
	{
		var table = document.getElementById('landRentTable');
		totalAmt = removeCommaFrmInput(document.getElementById('txtTotalAmountPayableOTR').value);
		var tempAmt = table.rows[rowno].cells[5].innerHTML;
		totalAmt =  parseInt(totalAmt,10) - parseInt(removeCommaFrmInput(tempAmt),10);
		document.getElementById('txtTotalAmountPayableOTR').value = convertToKenyanShillingFormatVal(totalAmt, true);
		amtToPaidLandRent=removeCommaFrmInput(document.getElementById("txtTotalAmountPaidOTR").value);
		amtToPaidLandRent= parseInt(amtToPaidLandRent,10)-parseInt(removeCommaFrmInput(table.rows[rowno].cells[6].innerHTML),10);
		document.getElementById('txtTotalAmountPaidOTR').value=convertToKenyanShillingFormatVal(amtToPaidLandRent, true);
		deleteFlagLandRent = 0;
		if(document.getElementById('hidIsPartial').value == "Y" || document.getElementById('hidIsPartial').value == "y" )
		{
			selectedFlagPartial=selectedFlagPartial-1;
		}
		currentPaymentLandRent();
	}
	else
	{
		deleteFlagLandRent = 0;
	}
}

function validateModifyRowIntolandRentTable(tab_str, cols_1, rowno, cols_2)
{
	modifyFlagLandRent = 1;
	commonModifyFlag=1;
	tempAmtVal = document.getElementById('in_landRentTable_5').value;
	TotalAmtToPaidLandRent=document.getElementById('in_landRentTable_6').value;
}


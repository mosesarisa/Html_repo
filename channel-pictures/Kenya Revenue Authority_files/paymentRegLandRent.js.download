					modify=trim(tabObj1.rows[i].cells[2].textContent,"");
				}
				if((trim(document.getElementById('id_temp_'+id+'_'+i+'_1').value))=="Delete" && (modify=="Modify") &&  (tabObj1.rows[i].cells[4].innerHTML==demandNoticeNumber))
				{
					alert(demandNoticeNoExists);
					return false;
				}
			}
		}

		return true;
	}

}

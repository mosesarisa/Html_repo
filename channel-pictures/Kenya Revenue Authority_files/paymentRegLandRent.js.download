
function addRentTable(row)
{

	resetErrorList();
	if (validateFormFields(paymentRegOTRLandRentDetailsJson) ) 
	{

		var checkDuplicateFlag = checkDuplicateLandRent(row,'landRentTable');
		if(checkDuplicateFlag==false)
		{
			return false;
		}
		if(removeCommaFrmInput(document.getElementById('in_landRentTable_8').value)<=0)
		{
			alert(minAnnualRate);
			return false;	
		}
		if(removeCommaFrmInput(document.getElementById('in_landRentTable_5').value)<=0)
		{
			alert(demadNoticeAmt);
			return false;
		}
		if(removeCommaFrmInput(document.getElementById('in_landRentTable_6').value)<=0)
		{
			alert(amtPaid);
			return false;
		}
		hideTab('TabView',5);
		resetErrorList();
		incError=0;
		if(addConfirm('a_landRentTable'))
		{
			if(document.getElementById('a_landRentTable').innerHTML!="Modify")
			{
				addrow('landRentTable', '4', '10','4','validateDeleteRowIntolandRentTable','validateModifyRowIntolandRentTable','N','N')
			}
			else
			{
				return true;
			}
		}
	//	return true;
	}
	else
	{
		getErrTabToggle('TabView','2');	
		tabview_switch('TabView', '2');
		hideAndShowTable("errorDiv");
		document.getElementById('btnTbl').style.display="none";
	}
}
function beforeAddIntolandRentTable(row)
{
	var checkDuplicateFlag = checkDuplicateLandRent(row,'landRentTable');
	if(document.getElementById('hidIsPartial').value != "Y")
	{
		if(checkDuplicateFlag){
			checkDuplicateFlag = isLandRentRegistered();
		}
	}
	if(checkDuplicateFlag==false)
	{
		return false;
	}
	else
	{
		document.getElementById("hidAmountForLocalUse").value = removeCommaFrmInput(document.getElementById("in_landRentTable_5").value); 
		TotalAmtToPaidLandRent=removeCommaFrmInput(document.getElementById('in_landRentTable_6').value);
		totalAmt=removeCommaFrmInput(document.getElementById('txtTotalAmountPayableOTR').value);
		amtToPaidLandRent=removeCommaFrmInput(document.getElementById("txtTotalAmountPaidOTR").value);
	}
}

function afterAddIntolandRentTable(){
	document.getElementById('landRentTable').style.display='';
	landRentTableCounter = landRentTableCounter+1;

	totalAmt = parseInt(totalAmt,10) +parseInt(removeCommaFrmInput(document.getElementById("hidAmountForLocalUse").value),10);
	document.getElementById('txtTotalAmountPayableOTR').value = convertToKenyanShillingFormatVal(totalAmt, true);
	amtToPaidLandRent = parseInt(amtToPaidLandRent,10) +parseInt(removeCommaFrmInput(TotalAmtToPaidLandRent),10);
	document.getElementById("txtTotalAmountPaidOTR").value =  convertToKenyanShillingFormatVal(amtToPaidLandRent, true);
	document.getElementById('in_landRentTable_10').selectedIndex=0;
	if(document.getElementById('hidIsPartial').value == "Y" || document.getElementById('hidIsPartial').value == "y" )
	{
		document.getElementById('landRentRadio_'+rowNumbmerPartial).checked=false;
		selectedFlagPartial=selectedFlagPartial+1;
	}

}

function beforeDeleteIntolandRentTable()
{
	if(modifyFlagLandRent==1)
	{
		alert(confirmModifyingOperation);
		return false;
	}
}

function afterDeleteIntolandRentTable()
{
	deleteFlagLandRent = 1;
	landRentTableCounter = landRentTableCounter -1;
}
function beforeModifyIntolandRentTable(row)
{
	if(addRentTable())
	{
		var checkDuplicateFlag = checkDuplicateLandRent(row,'landRentTable');
		if(checkDuplicateFlag==false)
		{
			return false;
		}
		else
		{
			totalAmt=removeCommaFrmInput(document.getElementById('txtTotalAmountPayableOTR').value);
			amtToPaidLandRent=removeCommaFrmInput(document.getElementById("txtTotalAmountPaidOTR").value);
		}
	}
	else
	{
		return false;
	}
}


function afterModifyIntolandRentTable()
{
	modifyFlagLandRent = 0;
	commonModifyFlag=0;
	var table = document.getElementById('landRentTable');  
	totalAmt = parseInt(removeCommaFrmInput(totalAmt),10) + parseInt(removeCommaFrmInput(table.rows[rowno].cells[5].innerHTML),10) - parseInt(removeCommaFrmInput(tempAmtVal),10);
	amtToPaidLandRent = parseInt(removeCommaFrmInput(amtToPaidLandRent),10) + parseInt(removeCommaFrmInput(table.rows[rowno].cells[6].innerHTML),10) - parseInt(removeCommaFrmInput(TotalAmtToPaidLandRent),10);
	document.getElementById('txtTotalAmountPayableOTR').value = convertToKenyanShillingFormatVal(totalAmt, true);
	document.getElementById('txtTotalAmountPaidOTR').value=convertToKenyanShillingFormatVal(amtToPaidLandRent, true);

function clearFielsCGTInterestFieldSet()
{
	document.getElementById("periodYear").selectedIndex=0;
	document.getElementById("periodMonth").selectedIndex=0;
	
}


function checkAmountIntPayReg()
{
	var amountToBePaid=document.getElementById("in_cgtInterestTable_6").value;
	var result=convertToKenyanShillingFormatVal(amountToBePaid,true);
	document.getElementById("in_cgtInterestTable_6").value=result;
	
	
}


function beforeModifyClickedFunctionInterest(){
	var amountPayable=parseFloat(removeCommaFrmInput(document.getElementById('in_cgtInterestTable_5').value));
	var amount=parseFloat(removeCommaFrmInput(document.getElementById("in_cgtInterestTable_6").value));
	
	if(isNaN(amount))
	{
		alert("Please enter valid amount to be paid.");
		return false;
	}
	else if(amount<0 || amount == "")
		{
			alert("Amount must be greater than 0");
			return false;
		}
	else if(amount>amountPayable)
		{
			alert("Amount Should be less than amount Payable");
			return false;
		}
	return true;

}
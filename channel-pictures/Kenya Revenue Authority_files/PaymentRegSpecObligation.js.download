	effectiveYear=parseInt(effectiveDate.substr(6, effectiveDate.length),10);
	var curdate = new Date();
	var curyear = parseInt(curdate.getFullYear());
	var noOfYears=parseInt(curyear,10)-parseInt(effectiveYear,10)+1;
	GetYearsOfDemandForLandRentWeb.getYearsOfDemandForLandRentWeb (effectiveDate,noOfYears,function(data) {

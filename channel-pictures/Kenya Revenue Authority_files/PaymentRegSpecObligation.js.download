			//	fetchSplOblHdrWebByObligation("Y");

		}
		else if(specObligId == sugarObligationId){
			document.getElementById('in_sugarTable_12').value=frmDate;
			document.getElementById('in_sugarTable_13').value=toDate;
			var checkDuplicateFlag=checkDuplicateForSugar(row);
